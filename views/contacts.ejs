<!--get access to header file-->
<% include ../partials/header %>
            <h1>
                Contact us!
            </h1>
 
        
        <div class="row">
            <div class="grid-container">
              
            <% contact.forEach(function(contact) { %>
              
                <div class="item">     
                    <div class="item">
                        <h1 class="itemtitle">Street: <%= contact.street%></h1>
                        <h1 class="itemp">City: <%= contact.city%></h1>
                        <h1 class="itemp">Country: <%= contact.country%></h1>
                        <h1 class="itemp">Hours: <%= contact.hours%></h1>
                        <h1 class="itemp">Phone: <%= contact.phone%></h1>
                        <h1 class="itemp">Fax: <%= contact.fax%></h1>
                        <h1 class="itemp">Email: <%= contact.email%></h1>
                        <a href="/delete_contacts/"<%= contact.id %> class="button_delete">Delete</a>
                        <a href="/editcontact/<%= contact.id %>" class="button_edit">Edit</a>
                    </div>
                </div>
                
            <% }); %>   
                   
            </div>
          </h1>
        </div>
        
        
<!--get access to footer file-->
<% include ../partials/footer %>